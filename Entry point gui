-- Scripts:

local function TUNEPG_fake_script() -- ControlRoom.LocalScript 
	local script = Instance.new('LocalScript', ControlRoom)

	
	script.Parent.Activated:Connect(function()
		game.Players.LocalPlayer.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
		wait(0.1)
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-10.769, 15.86, 51.474)
	end)
		
	
	
end
coroutine.wrap(TUNEPG_fake_script)()
local function FTTWTO_fake_script() -- CLOSE.LocalScript 
	local script = Instance.new('LocalScript', CLOSE)

	script.Parent.Activated:Connect(function()
		script.Parent.Parent.Visible = false
		script.Parent.Parent.Parent.MainGUI.Visible = true
	end)
end
coroutine.wrap(FTTWTO_fake_script)()
